{
  "manifest_version": 2,
  "version": "0.3.0",
  "name": "pixelUP",
  "icons": { "500": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["*"],
  "permissions": ["storage"]
}
