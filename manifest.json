{
  "manifest_version": 2,
  "version": "0.2.0",
  "name": "pixelUP",
  "icons": {
    "500": "icon.svg"
  },
  "page_action": {
    "default_icon": "icon.svg",
    "default_popup": "popup.html",
    "show_matches": [
      "https://www.youtube.com/*",
      "https://www.twitch.tv/*"
    ]
  },
  "content_scripts": [{
    "js": [
      "upscale.js"
    ],
    "matches": [
      "https://www.youtube.com/*",
      "https://www.twitch.tv/*"
    ]
  }],
  "web_accessible_resources": [],
  "permissions": ["storage"]
}
